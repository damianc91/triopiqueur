        </ComboBox>

        <TextBlock Grid.Row="4" Grid.Column="0"  Text="Prix max :" HorizontalAlignment="Right" Foreground="White" Margin="0,0,0,0" VerticalAlignment="Center"/>
        <ComboBox Grid.Row="4" Grid.Column="1" Name="comboBoxPrixMax" SelectionChanged="Sort" Background="#FFD68712"  Foreground="White" SelectedIndex="0"  HorizontalAlignment="Stretch"  ItemContainerStyle="{StaticResource MyComboBoxStyle}" HorizontalContentAlignment="Center">
            <ComboBoxItem>10 000 000</ComboBoxItem>
            <ComboBoxItem>2 000 000</ComboBoxItem>
            <ComboBoxItem>1 500 000</ComboBoxItem>
            <ComboBoxItem>1 000 000</ComboBoxItem>
