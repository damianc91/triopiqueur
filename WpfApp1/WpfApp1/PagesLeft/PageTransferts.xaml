                                </StackPanel>
                            </DataTemplate>
                        </GridViewColumn.CellTemplate>
                    </GridViewColumn>
                    <GridViewColumn   Header="VAL"  HeaderContainerStyle="{StaticResource MyHeaderStyle}">
                        <GridViewColumn.CellTemplate>
                            <DataTemplate>
                                <StackPanel  Orientation="Horizontal" Height="25" HorizontalAlignment="Center">
                                    <TextBlock Text="{Binding valRound}" ToolTip="{Binding val}" Foreground="{Binding valColor}"  FontSize="11" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                </StackPanel>
                            </DataTemplate>
                        </GridViewColumn.CellTemplate>
                    </GridViewColumn>
                    <GridViewColumn  Header="PAV"  HeaderContainerStyle="{StaticResource MyHeaderStyle}">
                        <GridViewColumn.CellTemplate>
                            <DataTemplate>
                                <StackPanel  Orientation="Horizontal" Height="25" HorizontalAlignment="Center">
                                    <TextBlock Text="{Binding pavRound}" ToolTip="{Binding pav}" Foreground="{Binding pavColor}"  FontSize="11" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                </StackPanel>
                            </DataTemplate>
                        </GridViewColumn.CellTemplate>
                    </GridViewColumn>
                    <GridViewColumn   Header="AGI"  HeaderContainerStyle="{StaticResource MyHeaderStyle}">
                        <GridViewColumn.CellTemplate>
                            <DataTemplate>
                                <StackPanel  Orientation="Horizontal" Height="25" HorizontalAlignment="Center">
                                    <TextBlock Text="{Binding agiRound}" ToolTip="{Binding agi}" Foreground="{Binding agiColor}"  FontSize="11" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                </StackPanel>
                            </DataTemplate>
                        </GridViewColumn.CellTemplate>
                    </GridViewColumn>
                    <GridViewColumn  Header="CLM" HeaderContainerStyle="{StaticResource MyHeaderStyle}">
                        <GridViewColumn.CellTemplate>
                            <DataTemplate>
                                <StackPanel  Orientation="Horizontal" Height="25" HorizontalAlignment="Center">
                                    <TextBlock Text="{Binding clmRound}"  ToolTip="{Binding clm}" Foreground="{Binding clmColor}"  FontSize="11" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                </StackPanel>
                            </DataTemplate>
                        </GridViewColumn.CellTemplate>
                    </GridViewColumn>
                    <GridViewColumn  Header="SPR"  HeaderContainerStyle="{StaticResource MyHeaderStyle}">
                        <GridViewColumn.CellTemplate>
                            <DataTemplate>
                                <StackPanel  Orientation="Horizontal" Height="25" HorizontalAlignment="Center">
                                    <TextBlock Text="{Binding sprRound}"  ToolTip="{Binding spr}" Foreground="{Binding sprColor}"  FontSize="11" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                </StackPanel>
                            </DataTemplate>
                        </GridViewColumn.CellTemplate>
                    </GridViewColumn>
                    <GridViewColumn   Header="END"  HeaderContainerStyle="{StaticResource MyHeaderStyle}">
                        <GridViewColumn.CellTemplate>
                            <DataTemplate>
                                <StackPanel  Orientation="Horizontal" Height="25" HorizontalAlignment="Center" >
                                    <TextBlock Text="{Binding endRound}" ToolTip="{Binding end}" Foreground="{Binding endColor}"  FontSize="11" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                </StackPanel>
                            </DataTemplate>
                        </GridViewColumn.CellTemplate>
                    </GridViewColumn>
                    <GridViewColumn   Header="RES"  HeaderContainerStyle="{StaticResource MyHeaderStyle}">
                        <GridViewColumn.CellTemplate>
                            <DataTemplate>
                                <StackPanel Orientation="Horizontal" Height="25" HorizontalAlignment="Center">
                                    <TextBlock Text="{Binding resRound}" ToolTip="{Binding res}" Foreground="{Binding resColor}"  FontSize="11" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                </StackPanel>
                            </DataTemplate>
                        </GridViewColumn.CellTemplate>
                    </GridViewColumn>
                    <GridViewColumn  Header="PRIX"  HeaderContainerStyle="{StaticResource MyHeaderStyle}">
                        <GridViewColumn.CellTemplate>
                            <DataTemplate>
                                <StackPanel Orientation="Horizontal" Height="25" HorizontalAlignment="Center">
                                    <TextBlock Text="{Binding prix}" Foreground="White" FontSize="11" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                </StackPanel>
                            </DataTemplate>
                        </GridViewColumn.CellTemplate>
                    </GridViewColumn>
                    <GridViewColumn  Header="J" HeaderContainerStyle="{StaticResource MyHeaderStyle}">
                        <GridViewColumn.CellTemplate>
                            <DataTemplate>
                                <StackPanel Orientation="Horizontal" Height="25" HorizontalAlignment="Center">
                                    <TextBlock Text="{Binding jours}" Foreground="White" FontSize="11" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                </StackPanel>
                            </DataTemplate>
                        </GridViewColumn.CellTemplate>
                    </GridViewColumn>
                </GridView>
            </ListView.View>
        </ListView>

        <TextBlock Grid.Row="2" Grid.Column="0"  Text="Spécialité :" HorizontalAlignment="Right" Foreground="White" Margin="0,5,0,0"/>
        <ComboBox Grid.Row="2" Grid.Column="1" Name="comboBoxSpecialite" SelectionChanged="Sort"  Background="#FFD68712"  Foreground="White" SelectedIndex="0" HorizontalAlignment="Stretch" ItemContainerStyle="{StaticResource MyComboBoxStyle}" HorizontalContentAlignment="Center">
            <ComboBoxItem>All</ComboBoxItem>
            <ComboBoxItem>Baroudeur</ComboBoxItem>
            <ComboBoxItem>Grimpeur</ComboBoxItem>
            <ComboBoxItem>Vallon</ComboBoxItem>
            <ComboBoxItem>Pavé</ComboBoxItem>
            <ComboBoxItem>Cyclo cross</ComboBoxItem>
            <ComboBoxItem>CLM</ComboBoxItem>
            <ComboBoxItem>Omnium</ComboBoxItem>
        </ComboBox>

        <TextBlock Grid.Row="3" Grid.Column="0"  Text="Age max :" HorizontalAlignment="Right" Foreground="White" Margin="0,6,0,0"/>
        <ComboBox Grid.Row="3" Grid.Column="1"  Name="comboBoxAgeMax" SelectionChanged="Sort"  Background="#FFD68712"  Foreground="White" SelectedIndex="0" HorizontalAlignment="Stretch"  ItemContainerStyle="{StaticResource MyComboBoxStyle}" HorizontalContentAlignment="Center">
