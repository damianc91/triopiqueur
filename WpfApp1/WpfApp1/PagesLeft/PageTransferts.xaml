                        </materialDesign:PackIcon.Foreground>
                    </materialDesign:PackIcon>

                    <materialDesign:PackIcon Kind="star" Grid.Column="1"  Foreground="Black"   Width="25" Height="25" />
                    <materialDesign:PackIcon Kind="None" Grid.Column="1"  Width="25" Height="25"  >
                        <materialDesign:PackIcon.Foreground>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
