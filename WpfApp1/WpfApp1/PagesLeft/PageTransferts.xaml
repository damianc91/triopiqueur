            <ComboBoxItem>250 000</ComboBoxItem>
            <ComboBoxItem>100 000</ComboBoxItem>
            <ComboBoxItem>1</ComboBoxItem>
        </ComboBox>

        <TextBlock  Grid.Row="5" Grid.Column="0"  Text="Potentiel min :" HorizontalAlignment="Right" Foreground="White" Margin="0,2,0,0" VerticalAlignment="Center"/>
        <ComboBox  Grid.Row="5" Grid.Column="1" Name="comboBoxPotentielMin"  SelectionChanged="Sort" Background="#FFD68712"  Foreground="White" SelectedIndex="0" HorizontalAlignment="Stretch"  ItemContainerStyle="{StaticResource MyComboBoxStyle}" HorizontalContentAlignment="Center">
            <ComboBoxItem  Name="comboPot1">
                <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <materialDesign:PackIcon Kind="star" Grid.Column="0"  Foreground="Black"   Width="25" Height="25" />
                    <materialDesign:PackIcon Kind="StarOutline" Grid.Column="0"  Width="25" Height="25">
                        <materialDesign:PackIcon.Foreground>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFE5D519" Offset="0"/>
                                <GradientStop Color="#FFD68712" Offset="1"/>
                            </LinearGradientBrush>
                        </materialDesign:PackIcon.Foreground>
                    </materialDesign:PackIcon>
                    <materialDesign:PackIcon Kind="StarHalf" Grid.Column="0"  Foreground="Black"   Width="25" Height="25" RenderTransformOrigin="0.5,0.5">
                        <materialDesign:PackIcon.RenderTransform>
                            <TransformGroup>
                                <ScaleTransform ScaleY="1" ScaleX="-1"/>
                                <SkewTransform AngleY="0" AngleX="0"/>
                                <RotateTransform Angle="0"/>
                                <TranslateTransform/>
                            </TransformGroup>
                        </materialDesign:PackIcon.RenderTransform>
                    </materialDesign:PackIcon>

                    <materialDesign:PackIcon Kind="star" Grid.Column="1"  Foreground="Black"   Width="25" Height="25" />
                    <materialDesign:PackIcon Kind="None" Grid.Column="1"  Width="25" Height="25"  >
                        <materialDesign:PackIcon.Foreground>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
